# Environment configuration for the application
# Save this file as .env in the root directory of your project
# Ensure this file is excluded from version control for security reasons

# ------------------------------
# Steam API Configuration
# ------------------------------

# Steam API Key for accessing Steam services
# Get your Steam API key from https://steamcommunity.com/dev/apikey
STEAM_API_KEY=your_steam_api_key

# ------------------------------
# Redis Configuration
# ------------------------------

# Redis server hostname or IP address
REDIS_HOST=localhost

# Redis server port (default is 6379)
REDIS_PORT=6379